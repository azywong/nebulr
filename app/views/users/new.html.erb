<%= form_for @user, url: {action: "create"}, html: {class: "question_form"} do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br />
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Create Account" %>
<% end %>